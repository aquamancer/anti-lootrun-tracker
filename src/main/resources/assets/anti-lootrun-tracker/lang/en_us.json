  "text.autoconfig.anti-lootrun-tracker.option.mobListOptions": "Mob List Options",
  "text.autoconfig.anti-lootrun-tracker.option.mobListOptions.@Tooltip": "Render mob list in the action bar when\nlooking directly at a chest",
  "text.autoconfig.anti-lootrun-tracker.option.mobListOptions.mobListEnabled": "Enabled",
  "text.autoconfig.anti-lootrun-tracker.option.mobListOptions.mobListEnabled.@Tooltip": "Disable the mob list",
